inicio {
	entero n,curr;
  real prueba;
  
  prueba=1.0-3.4*2.0/5.0;
  imprimir "Prueba es " prueba;
  
	imprimir "Introduzca un numero: \n";
	introducir n;
	curr=2;
	imprimir "Sus factores primos son: \n";
	mientras(curr<=n){
		entero d;
		d=n/curr;
		si((d*curr)==n){
			imprimir curr;
			imprimir " ";
			n=n/curr;
		}
		en otro caso{
			curr=curr+1;
		}
	}
	imprimir "1\n";
}
